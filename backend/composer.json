{
  "name": "bcms/backend",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.2",
    "laravel/horizon": "^5.42",
    "laravel/octane": "^2.13",
    "spiral/roadrunner": "^2025.1.6",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php artisan key:generate --ansi || true"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "audit": {
      "block-insecure": false
    }
  }
}